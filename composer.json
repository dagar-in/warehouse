{
    "name": "dagar/warehouse",
    "description": "A set of commands & packages for laravel use to help in development.",
    "keywords": [
        "dagar",
        "warehouse",
        "pint",
        "dbm"
    ],
    "homepage": "https://github.com/dagar/warehouse",
    "license": "MIT",
    "authors": [
        {
            "name": "Ankit",
            "email": "ankit@dagar.in",
            "homepage": "https://dagar.in",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "dagar/crawler": "dev-master",
        "illuminate/console": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Dagar\\Warehouse\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dagar\\Warehouse\\Providers\\Warehouse"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
